    "github": {
      "url": "https://github.com/yjtmD/wx-notes"
    },
    "donate": {
      "wechat": "http://www.wxylx.top/images/wechat.jpg",
      "alipay": "http://www.wxylx.top/images/alipay.jpg",
      "title": "",
      "button": "打赏",
      "alipayText": "支付宝捐赠",
      "wechatText": "微信捐赠"
    },
    "anchor-navigation-ex": {
      "associatedWithSummary":false,
      "showLevel":true,
      "multipleH1": true,
      "mode": "float"
    },
    "fontsettings": {
      "theme": "white"
